#include <stdio.h>

int main() {
  
    printf("--- For Loop ---\n");
    for (int i = 1; i <= 5; i++) {
        printf("%d ", i);
    }
    printf("\n\n");

   
    printf("--- While Loop ---\n");
    int j = 5;
    while (j >= 1) {
        printf("%d ", j);
        j--;
    }
    printf("\n\n");

   
    printf("--- Do-While Loop ---\n");
    int k = 1;
    do {
        printf("%d ", k);
        k++;
    } while (k <= 3);
    printf("\n");

    return 0;
}
